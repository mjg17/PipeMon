[% meta.title = species _ ": analysis " _ analysis.logic_name _ " (" _ db_type _ ")"
-%]

<dl class="analysis">
[% FOREACH key IN keys -%]
 <dt>[% key %]</dt>
 <dd>[% analysis.$key || '&nbsp;' %]</dd>
[% END %]
[% IF db_type == 'pipe' -%]
 <dt>Jobs</dt>
 <dd>[% analysis.job_count %]</dd>
[% END %]
</dl>
